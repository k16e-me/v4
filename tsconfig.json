{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": false,
    "checkJs": false,
    "skipLibCheck": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
        "@atoms/*": ["src/atoms/*"],
        "@blocks/*": ["src/blocks/*"],
        "@layouts/*": ["src/layouts/*"],
        "@composites/*": ["src/composites/*"],
        "@content/*": ["src/content/*"],
        "@scripts/*": ["src/scripts/*"],
        "@styles/*": ["src/styles/*"],
        "@uploads/*": ["src/uploads/*"],
        "@meta/*": ["src/meta/*"],
    }
  }
}